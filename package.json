{
  "name": "jaoweb-community-data",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "_dev": "nuxt",
    "_build": "nuxt build",
    "_start": "nuxt start",
    "dev": "node -r tsconfig-paths/register ./node_modules/nuxt/bin/nuxt.js",
    "build": "node -r tsconfig-paths/register ./node_modules/nuxt/bin/nuxt.js build",
    "start": "node -r tsconfig-paths/register ./node_modules/nuxt/bin/nuxt.js start",
    "generate": "node -r tsconfig-paths/register ./node_modules/nuxt/bin/nuxt.js generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.13.6",
    "core-js": "3.30.1",
    "nuxt": "2.16.3",
    "vue": "2.6.14",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14",
    "vuetify": "2.6.15",
    "webpack": "5.79.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.3",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "3.0.1",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/eslint-module": "4.0.2",
    "@nuxtjs/recaptcha": "1.1.2",
    "@nuxtjs/stylelint-module": "5.0.1",
    "@nuxtjs/vuetify": "1.12.3",
    "@types/cors": "2.8.13",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-vue": "8.7.1",
    "express": "4.18.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "mysql2": "3.2.1",
    "nuxt-clipboard2": "0.2.1",
    "postcss-html": "1.5.0",
    "prettier": "2.8.7",
    "stylelint": "15.5.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "32.0.0"
  }
}
